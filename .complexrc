{
  "output": "./reports/complexity-report.md",
  "format": "markdown",
  "allfiles": false,
  "ignoreerrors": true,
  "excludepattern": "(node_modules)|(views)",
  "filepattern": "\\.js$",
  "maxfiles": 256,
  "silent": false,
  "logicalor": false,
  "switchcase": false,
  "forin": false,
  "trycatch": false,
  "newmi": false,
  "maxcyc": 10,
  "maxcycden": 100,
  "maxhd": 20,
  "maxhv": 500,
  "maxhe": 2000,
  "minmi": 100,
  "maxfod": 20,
  "maxcost": 50,
  "maxsize": 25
}
